       hasxlo=0
       hasxhi=0
       hasylo=0
       hasyhi=0
       if(iprocx.eq.1)then
c          ixlo=1
          hasxlo=1
       endif
       if(iprocx.eq.XPROCS)then
c          ixhi=nxlocal
          hasxhi=1
       endif
#ifndef YPERIODIC
       if(iprocy.eq.1) then
c          iylo=1
          hasylo=1
       endif
       if(iprocy.eq.YPROCS)then
c          iyhi=nylocal
          hasyhi=1
       endif
#endif
